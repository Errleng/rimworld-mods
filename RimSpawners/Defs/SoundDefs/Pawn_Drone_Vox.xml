<?xml version="1.0" encoding="utf-8"?><Defs>

<!-- Homer drone -->
  <SoundDef>
    <defName>Pawn_Drone_Homer_Wounded</defName>  
    <context>MapOnly</context>  
    <maxVoices>1</maxVoices>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Pawn/Drone/Drone_Homer_Wounded</clipFolderPath>
          </li>
        </grains>      
        <volumeRange>30~30</volumeRange>      
        <pitchRange>0.9311765~1.045882</pitchRange>      
        <repeatMode>NeverTwice</repeatMode>      
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

