<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Exotic -->
	<Operation Class="PatchOperationAdd">
		<xpath>*/TraderKindDef[defName="Orbital_Exotic"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_Tag">
				<tradeTag>Spawners</tradeTag>
				<thingDefCountRange>
					<min>-1</min>
					<max>1</max>
				</thingDefCountRange>
				<price>Expensive</price>
				<totalPriceRange>
					<min>2500</min>
					<max>10000</max>
				</totalPriceRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>Spawners</tradeTag>
				<thingDefCountRange>
					<min>-1</min>
					<max>1</max>
				</thingDefCountRange>
				<price>Cheap</price>
				<totalPriceRange>
					<min>2000</min>
					<max>4000</max>
				</totalPriceRange>
			</li>
		</value>
	</Operation>

